!!! html
%html
  %head
    %meta{:charset => "utf-8"}
    
    %title= data.page.title 
    = stylesheet_link_tag "application"
  
  %body
    .wrapper
      = partial "layouts/partials/header"
      #main
        %h1.page-title= data.page.title
        %h5.date= date current_page
        = yield 
  = javascript_include_tag  "all"
